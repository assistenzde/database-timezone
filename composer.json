{
    "name": "assistenzde/database-timezone",
    "type": "symfony-bundle",
    "homepage": "https://github.com/assistenzde/database-timezone",
    "description": "Unified database time zone values",
    "keywords": [
        "database",
        "doctrine",
        "date",
        "datetime"
    ],
    "license": "mit",
    "authors": [
        {
            "name": "Falko Matthies",
            "email": "falko.m@web.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "dama/doctrine-test-bundle": "^6.4",
        "symfony/yaml": "^5.1",
        "symfony/browser-kit": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Assistenzde\\DatabaseTimezoneBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Assistenzde\\DatabaseTimezoneBundle\\UnitTest\\": "tests/unit/"
        }
    }
}
